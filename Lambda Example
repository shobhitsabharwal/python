my_list=[1,5,4,6,8,11,3,12]
new_list=list(map(lambda x:x*2,my_list))
print(my_list)
print(new_list)


my_list1=[1,2,3,4,5,6,7]
my_list2=[4,7,8,9,2,3,4]

new_list1=list(map(lambda x,y:x+y,my_list1,my_list2))
print(new_list1)

##filter even number
my_list3=[1,5,6,7,8,10]
mew_list3=list(filter(lambda x:(x%2==0),my_list3)) ##filter will remove not satified element
print(mew_list3)

##converted to tuple
my_list4=[1,3,45,5,6,2]
new_list4=list(map(lambda x:(x,1),my_list4))
print(new_list4)
