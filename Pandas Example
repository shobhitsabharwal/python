import pandas
us_wars_by_end = {'day': ['1/1/2017', '1/2/2007', '1/3/2017'],
                  'temperature': [32, 35, 28],
                  'windspeed': [6, 7 , 2],
                  'event': ['Rain', 'Sunny', 'Snow']}
df = pandas.DataFrame(us_wars_by_end)
print(df)
print(df.columns)
print(df.event)
